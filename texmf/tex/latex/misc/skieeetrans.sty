]{skThmStyle}


\declaretheorem[
refname={axiom,axioms},
Refname={Axiom,Axioms}
]{axiom}

\declaretheorem[
style=skThmStyle,
refname={theorem,theorems},
Refname={Theorem,Theorems}
]{theorem}

% lemma will share the same counter as theorem
\declaretheorem[sibling=theorem,
style=skThmStyle,
refname={lemma,lemmas},
Refname={Lemma,Lemmas}
]{lemma}

\declaretheorem[sibling=theorem,
style=skThmStyle,
refname={proposition,propositions},
Refname={Proposition,Propositions}
]{proposition}

\declaretheorem[sibling=theorem,
style=skThmStyle,
refname={corollary,corollaries},
Refname={Corollary,Corollaries}]
{corollary}


\declaretheorem[
style=skDefStyle,
refname={definition,definitions},
Refname={Definition,Definitions}]
{definition}

